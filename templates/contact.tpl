<!DOCTYPE html   PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">  <head>    <title>Listing contacts</title>    <link rel="stylesheet" href="style/structure.css" type="text/css" media="screen">    <script type="text/javascript" src="js/jquery-1.6.4.min.js"></script>  </head>  <body>    <h2>Contacts      <font size='2'>    <a href='index.php?module=calendar&method=display'>Calendar Events</a>      <a href='index.php?module=abtask&method=display'>Tasks</a>    <a href='index.php?module=documentlibrary&method=df'>Document Library</a>    <a href='index.php?module=user&method=logout'>logout</a>    </font>    </h2>    <div>    <{$link}>    </div>    <div>    <{$results_count}>    </div>    <div>    <{$google_sync}>    </div>     <{foreach $results as $r}>   		<div class="entry <{if ($r['is_deleted'])}> deleted <{/if}>">			<div class="name"><{$r['name']}></div>			<div class="data">				<table>					<tr>						<td class="header">Organization:</td>						<td><{$r['orgName']}></td>					</tr>					<tr>						<td class="header">Organization Title:</td>						<td><{$r['orgTitle']}></td>					</tr>					<tr>						<td class="header">Email:</td>						<td><{$r['emailAddress']}></td>					</tr>					<tr>						<td class="header">Phone:</td>						<td><{$r['phoneNumber']}></td>					</tr>					<tr>						<td class="header">Web:</td>						<td><{$r['website']}></td>					</tr>					<tr>						<td class="header">googleID:</td>						<td><{get_google_id($r['googleID'])}></td>					</tr>					<tr>						<td class="header">updated:</td>						<td><{$r['updated']}></td>					</tr>				</table>			</div>		</div>		<{/foreach}>  </body></html>